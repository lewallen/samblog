tex2jax: {
  inlineMath: [
    [
      '$', '$'
    ],
    [
      "\\(", "\\)"
    ]
  ],
  processEscapes: true
  },
  tex: {
    packages: {
      '[+]': ['amsFonts']
    } ,
    macros: {
      RR: '{\\bf R}',
      R: "{\\mathbb R}",
      C: "{\\mathbb C}",
      p: "{\\mathcal P}" // P - rond
    }
  }
